    if (global.item != -1){
        if  (instance_place(mouse_x,mouse_y,obj_inventory_box)){
            var box = instance_place(mouse_x,mouse_y,obj_inventory_box);
            
            //make sure box is empty
            if (box.item==-1){
                //box the item
                box.item = global.item
                global.item = -1;
            }else{
                //drop item on groun
                instance_create(mouse_x,mouse_y,global.item);
                global.item=-1;
            }
        }else{
            //drop item on groun
            instance_create(mouse_x,mouse_y,global.item);
            global.item=-1;
        }
    }
}
